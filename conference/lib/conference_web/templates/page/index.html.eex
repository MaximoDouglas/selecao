<table class="table">
  <thead>
    <tr>
      <th>Palestra:</th>

      <th>Duração: </th>
    </tr>
  </thead>
  <tbody>
<%= for track <- @tracks do %>
    <tr>
      <td><%= Enum.at(track.events, 0).title %></td><td><%= Enum.at(track.events, 0).duration %></td>
    </tr>
<% end %>
  </tbody>
</table>
